{
    "version": 2,
    "builds": [
        {
            "src": "frontend/vite.config.js",
            "use": "@vercel/vite"
        },
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/dist/index.html"
        }
    ]
}