{
    "version": 2,
    "builds": [
        {
            "src": "frontend/dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/dist/index.html"
        }
    ]
}